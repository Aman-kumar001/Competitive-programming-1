#include<stdio.h>

int main(){

    int t;
    scanf("%d" , &t);
    
    while(t--){
        int n,k;
    scanf("%d%d" ,&n , &k);
    int a[n];
    int count=0;
    for(int i=0 ; i<n ; i++){
        scanf("%d" , &a[i]);
        if(a[i]<=0)count++;
    }
    (count>=k) ? (printf("NO")) : (printf("YES"));
    }
    return 0;

}
