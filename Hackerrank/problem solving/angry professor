#include<stdio.h>

int main(){

    int t;
    scanf("%d" , &t);
    
    while(t--){
        int n,k;             //initialising two variables n and k
    scanf("%d%d" ,&n , &k);  //scanning two variables n and k
    int a[n];                //initialising array
    int count=0;             //initialising count as zero
    for(int i=0 ; i<n ; i++){
        scanf("%d" , &a[i]);  //for loop to scan array elements
        if(a[i]<=0)count++;
    }
    (count>=k) ? (printf("NO")) : (printf("YES"));
    }
    return 0;

}
